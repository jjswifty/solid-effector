{
    "name": "effector-solid",
    "version": "1.0.0",
    "description": "Todo with effector and solid. Custom webpack",
    "author": "TG - @oldbruh",
    "license": "MIT",
    "private": true,
    "scripts": {
        "build": "yarn run check-types && cross-env NODE_ENV=production webpack --mode production",
        "dev": "cross-env NODE_ENV=development webpack serve --mode development",
        "check-types": "tsc --noEmit",
        "start": "yarn run dev -- --open",
        "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,css,json,md}\""
    },
    "dependencies": {
        "commitlint": "^17.3.0",
        "core-js": "^3.17.1",
        "regenerator-runtime": "^0.13.9",
        "solid-js": "^1.6.5"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@types/node": "^18.11.15",
        "@types/webpack": "^5.28.0",
        "babel-loader": "^9.1.0",
        "babel-preset-solid": "^1.6.3",
        "cross-env": "^7.0.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.2",
        "mini-css-extract-plugin": "^2.7.2",
        "prettier": "^2.8.1",
        "solid-refresh": "^0.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": "./prettier.json"
}
